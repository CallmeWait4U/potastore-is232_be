{
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit",
      "source.organizeImports": "explicit"
    },
    "eslint.workingDirectories": [
      "./"
    ],
    "search.exclude": {
      "**/node_modules": true,
      "**/dist": true
    },
    "[prisma]": {
      "editor.defaultFormatter": "Prisma.prisma"
    },
    "python.analysis.extraPaths": [
      "./src/py",
      "./src/py"
    ]
}